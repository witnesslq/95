<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="customer95" extends="json-default">
<interceptors>
    <interceptor-stack name="jsonStack">
    	<interceptor-ref name="defaultStack"></interceptor-ref>
        <interceptor-ref name="json">
            <param name="enableSMD">true</param>
        </interceptor-ref>
    </interceptor-stack>
</interceptors>

	<action name="import_customer" class="com.dhcc.bussiness.sxydidc.customer95.actions.CustomerAction">
		<result name="success"  type="dispatcher">
				/system/alarm/import_result.jsp
			</result>
		<result name="error"  type="dispatcher">
				/system/alarm/import_result.jsp
			</result>
	</action>
	
	<action name="export_customer" class="com.dhcc.bussiness.sxydidc.customer95.actions.CustomerAction">
		<result name="success"  type="dispatcher">
				/system/alarm/import_result.jsp
			</result>
		<result name="error"  type="dispatcher">
				/system/alarm/import_result.jsp
			</result>
	</action>
	</package>
</struts>
